{
  "name": "@kexi/uiflow",
  "version": "1.1.8",
  "description": "uiflow - the simplest way to write down your ui-flow diagram like markdown",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "test": "jest",
    "lint": "npx eslint",
    "build": "yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json"
  },
  "bin": {
    "uiflow": "./bin/uiflow"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hirokidaichi/uiflow.git"
  },
  "author": "hirokidaichi",
  "license": "MIT",
  "dependencies": {
    "option-parser": "^1.0.2",
    "spawn-stream": "^1.0.2",
    "string-to-stream": "^1.0.1",
    "through2": "^2.0.1"
  },
  "bugs": {
    "url": "https://github.com/hirokidaichi/uiflow/issues"
  },
  "homepage": "https://github.com/hirokidaichi/uiflow",
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "typescript": "*"
  }
}
